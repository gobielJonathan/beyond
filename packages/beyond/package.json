  "version": "0.0.1",
  "description": "Main Code in Beyond Framework",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm-run-all --parallel dev:client dev:server",
    "dev:client": "webpack serve --config ./webpack/client/webpack.dev.js",
    "dev:server": "npm-run-all --parallel watch:server watch:build",
    "watch:build": "webpack --watch --config ./webpack/server/webpack.prod.js",
    "watch:server": "nodemon \"./build/server/main.js\" --watch \"./build/server\" 3002",
    "build:client": "webpack --config ./webpack/client/webpack.prod.js",
    "build:server": "webpack --config ./webpack/server/webpack.prod.js",
    "build": "pnpm run build:client && pnpm run build:server",
    "build:start": "npm-run-all --parallel  node "
  },
